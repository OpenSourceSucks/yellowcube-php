	<xs:element name="Order">
		<xs:annotation>
			<xs:documentation>Bestellung</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="wab:OrderHeader"/>
				<xs:element ref="wab:PartnerAddress"/>
				<xs:element ref="wab:ValueAddedServices" maxOccurs="unbounded"/>
				<xs:element ref="wab:OrderPositions"/>
