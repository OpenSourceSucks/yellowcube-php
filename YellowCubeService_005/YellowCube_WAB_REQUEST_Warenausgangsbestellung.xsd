			<xs:enumeration value="IC"/>
			<xs:enumeration value="IE"/>
			<xs:enumeration value="IK"/>
			<xs:enumeration value="SA"/>
			<xs:enumeration value="SG"/>
			<xs:enumeration value="UC"/>
			<xs:enumeration value="VC"/>
		</xs:restriction>
	</xs:simpleType>
