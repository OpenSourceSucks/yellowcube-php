			<xs:documentation>FlieskommaZahl Bsp. 1234567890123.123</xs:documentation>
			<xs:appinfo>
				<altova:exampleValues>
					<altova:example value="1234567890123.123"/>
				</altova:exampleValues>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:totalDigits value="16"/>
			<xs:fractionDigits value="3"/>
		</xs:restriction>
	</xs:simpleType>
	<!--E-Mailadresse Validierung-->
	<xs:simpleType name="EmailAdress">
		<xs:annotation>
			<xs:documentation>E-Mailadresse Validierung</xs:documentation>
			<xs:appinfo>
				<altova:exampleValues>
					<altova:example value="a@b.ch"/>
					<altova:example value=""/>
				</altova:exampleValues>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="241"/>
			<xs:pattern value="([^@]+@[^\.]+\..+)?"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="DocStream" type="xs:base64Binary">
		<xs:annotation>
			<xs:documentation>Binäre Base64 codierte Filedaten</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="DocType">
		<xs:annotation>
			<xs:documentation>Typ des Lieferbeleges</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:normalizedString">
				<xs:maxLength value="2"/>
				<xs:whiteSpace value="collapse"/>
				<xs:enumeration value="LS"/>
				<xs:enumeration value="BL"/>
				<xs:enumeration value="BC"/>
				<xs:enumeration value="DN"/>
				<xs:enumeration value="IV"/>
				<xs:enumeration value="RG"/>
				<xs:enumeration value="FA"/>
				<xs:enumeration value="ZS"/>
				<xs:enumeration value="BP"/>
				<xs:enumeration value="BV"/>
				<xs:enumeration value="PF"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="DocMimeType">
		<xs:annotation>
			<xs:documentation>MIME-Typ als Extention des Lieferbeleges. [pdf|pcl] Inhalt des Streams muss mit der Extention übereinstimmen.</xs:documentation>
			<xs:appinfo>
				<altova:exampleValues>
					<altova:example value="pcl"/>
					<altova:example value="pdf"/>
				</altova:exampleValues>
			</xs:appinfo>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:length value="3"/>
				<xs:whiteSpace value="collapse"/>
				<xs:enumeration value="pdf"/>
				<xs:enumeration value="PDF"/>
				<xs:enumeration value="pcl"/>
				<xs:enumeration value="PCL"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
</xs:schema>
