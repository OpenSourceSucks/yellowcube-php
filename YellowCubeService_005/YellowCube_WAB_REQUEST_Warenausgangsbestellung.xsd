	<xs:element name="OrderHeader">
		<xs:annotation>
			<xs:documentation>Bestellung</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="DepositorNo">
					<xs:annotation>
						<xs:documentation>Distanzhändler-ID gemäss Anmeldung Yellow-Cube</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:minLength value="1"/>
							<xs:maxLength value="10"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="CustomerOrderNo">
					<xs:annotation>
						<xs:documentation>Bestellnummer des Kunden (externe AuftragsReferenz-Nummer)</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:normalizedString">
							<xs:minLength value="1"/>
							<xs:maxLength value="35"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="CustomerOrderDate">
					<xs:annotation>
						<xs:documentation>Bestelldatum des Kunden im Format: YYYYMMDD</xs:documentation>
						<xs:appinfo>
							<altova:exampleValues>
								<altova:example value="20131027"/>
							</altova:exampleValues>
						</xs:appinfo>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="8"/>
							<xs:pattern value="(2[0-9][0-9][0-9])((0[0-9])|(1[0-2]))(([0-2][0-9])|(3[0-1]))"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="PartnerAddress">
		<xs:annotation>
			<xs:documentation>Adresse</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="wab:Partner" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Partner">
		<xs:annotation>
			<xs:documentation>Adresse</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="PartnerType">
					<xs:annotation>
						<xs:documentation>WE=Warenempfänger (Im Standard YellowCube wird nur eine Partnerrolle neben dem Aufgeber unterstützt.</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:normalizedString">
							<xs:maxLength value="2"/>
							<xs:minLength value="2"/>
							<xs:enumeration value="WE"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="PartnerNo">
					<xs:annotation>
						<xs:documentation>Kundennummer des Empfängers. Ist zusammen mit der ID des Distanzhändlers (DepositorNo) eindeutig.</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:normalizedString">
							<xs:maxLength value="10"/>
							<xs:minLength value="1"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="PartnerReference" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Referenz des Partners</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="50"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="Title" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Anrede</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:normalizedString">
							<xs:maxLength value="15"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="Name1">
					<xs:annotation>
						<xs:documentation>Name</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:normalizedString">
							<xs:maxLength value="35"/>
							<xs:minLength value="1"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="Name2" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Name2 Zusatz</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:normalizedString">
							<xs:maxLength value="35"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="Name3" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Name3 Zusatz</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:normalizedString">
							<xs:maxLength value="35"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="Name4" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Name4 Zusatz</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:normalizedString">
							<xs:maxLength value="35"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="Street">
					<xs:annotation>
						<xs:documentation>Strasse inkl. Hausnummer</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:normalizedString">
							<xs:maxLength value="35"/>
							<xs:minLength value="1"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="CountryCode">
					<xs:annotation>
						<xs:documentation>Ländercode nach ISO-CountryCode ISO 3166-1</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:normalizedString">
							<xs:maxLength value="2"/>
							<xs:minLength value="2"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="ZIPCode">
					<xs:annotation>
						<xs:documentation>Postleitzahl, in der Schweiz 4-stellig.</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:normalizedString">
							<xs:maxLength value="10"/>
							<xs:minLength value="4"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="City">
					<xs:annotation>
						<xs:documentation>Ort</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:normalizedString">
							<xs:maxLength value="35"/>
							<xs:minLength value="1"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="POBox" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Postfach</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:normalizedString">
							<xs:maxLength value="10"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="PhoneNo" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Telefon Nummer</xs:documentation>
						<xs:appinfo>
							<altova:exampleValues>
								<altova:example value="+41 58 338 11 11"/>
								<altova:example value="0041583381111"/>
								<altova:example value="+41 58 338 11 11"/>
								<altova:example value="058 338 11 11"/>
								<altova:example value=""/>
							</altova:exampleValues>
						</xs:appinfo>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:normalizedString">
							<xs:maxLength value="16"/>
							<xs:pattern value="([+]?[0-9\s]{6,16})?"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="MobileNo" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Mobile Nummer</xs:documentation>
						<xs:appinfo>
							<altova:exampleValues>
								<altova:example value="+41 79 338 11 11"/>
								<altova:example value="0041793381111"/>
								<altova:example value="+41 79 338 11 11"/>
								<altova:example value="079 338 11 11"/>
								<altova:example value=""/>
							</altova:exampleValues>
						</xs:appinfo>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="16"/>
							<xs:pattern value="([+]?[0-9\s]{6,16})?"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="SMSAvisMobNo" minOccurs="0">
					<xs:annotation>
						<xs:documentation>SMS Mobile Nummer falls für SMS-Avisierung vorgesehen, bitte hier allenfalls wiederholen.
Mobile-Nr.:0041793020467 ohne Lehrschlag</xs:documentation>
						<xs:appinfo>
							<altova:exampleValues>
								<altova:example value="0041793020467"/>
							</altova:exampleValues>
						</xs:appinfo>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:normalizedString">
							<xs:maxLength value="16"/>
							<xs:pattern value="([0-9]{9,16})?"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="FaxNo" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Fax Nummer. Falls für Fax-Avisierung vorgesehen, bitte Format genau prüfen.
Fax-Nr.:0041583381112 ohne Lehrschlag</xs:documentation>
						<xs:appinfo>
							<altova:exampleValues>
								<altova:example value="0041583389999"/>
							</altova:exampleValues>
						</xs:appinfo>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:normalizedString">
							<xs:maxLength value="16"/>
							<xs:pattern value="([0-9]{9,16})?"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="Email" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Email-Adresse des Partners</xs:documentation>
						<xs:appinfo>
							<altova:exampleValues>
								<altova:example value="a@b.ch"/>
								<altova:example value=""/>
							</altova:exampleValues>
						</xs:appinfo>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="wab:EmailAdress">
							<xs:maxLength value="241"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="LanguageCode">
					<xs:annotation>
						<xs:documentation>Sprachcode</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:normalizedString">
							<xs:enumeration value="de"/>
							<xs:enumeration value="fr"/>
							<xs:enumeration value="it"/>
							<xs:enumeration value="en"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ValueAddedServices">
		<xs:annotation>
			<xs:documentation>Zusatzleistungen</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="wab:AdditionalService"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="AdditionalService">
		<xs:annotation>
			<xs:documentation>Zusatzleistungen</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="BasicShippingServices">
					<xs:annotation>
						<xs:documentation>Basis-Produkte für YellowCube Versand: &quot;ECO&quot;, &quot;PRI&quot;, &quot;ECO LKW&quot;, &quot;PICKUP&quot; (Selbstabholung), &quot;LKW EXPRESS&quot;, &quot;SKB&quot;, &quot;SP SEM&quot; etc.</xs:documentation>
						<xs:appinfo>
							<altova:exampleValues>
								<altova:example value="PRI"/>
								<altova:example value="ECO"/>
								<altova:example value="SEM"/>
								<altova:example value="RETOURE"/>
							</altova:exampleValues>
						</xs:appinfo>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:normalizedString">
							<xs:maxLength value="40"/>
							<xs:minLength value="1"/>
							<xs:enumeration value="RETURN"/>
							<xs:enumeration value="RETOURE"/>
							<xs:enumeration value="PRI"/>
							<xs:enumeration value="ECO"/>
							<xs:enumeration value="SP ECO"/>
							<xs:enumeration value="SP PRI"/>
							<xs:enumeration value="PPR"/>
							<xs:enumeration value="SEM"/>
							<xs:enumeration value="SP SEM"/>
							<xs:enumeration value="SKB"/>
							<xs:enumeration value="SP SKB"/>
							<xs:enumeration value="VL"/>
							<xs:enumeration value="PICKUP"/>
							<xs:enumeration value="LKW ECO"/>
							<xs:enumeration value="LKW PRIO"/>
							<xs:enumeration value="LKW EXPRESS"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="AdditionalShippingServices" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Zusatzleistungen gemäss Postpakete Postlogistics:
SI; AS; BLN; SA; AZS; etc. (BLN beinhaltet SI, deshalb nur BLN alleine übermitteln)</xs:documentation>
						<xs:appinfo>
							<altova:exampleValues>
								<altova:example value="SI"/>
								<altova:example value="AS"/>
								<altova:example value="BLN"/>
								<altova:example value="SA"/>
								<altova:example value="AZS"/>
								<altova:example value="SI;SA"/>
								<altova:example value="SI;AZS"/>
							</altova:exampleValues>
						</xs:appinfo>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:normalizedString">
							<xs:maxLength value="40"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="DeliveryInstructions" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Zustellanweisungen an die Paket-Zustellung gemäss [WSBC] Bsp. ZAW3213;ZAW3219 (Max 5)</xs:documentation>
						<xs:appinfo>
							<altova:exampleValues>
								<altova:example value="ZAW3213"/>
								<altova:example value="ZAW3219"/>
								<altova:example value="ZAW3213;ZAW3219"/>
							</altova:exampleValues>
						</xs:appinfo>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:normalizedString">
							<xs:maxLength value="40"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="FloorNo" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Stockwerk-Bezeichnung für Auslieferung, kann für Paket definiert werden. (wird nicht auf Paket-Etikette gedruckt)</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:normalizedString">
							<xs:maxLength value="35"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="NotificationType" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Medium-Typ der Avisierung
[1=TEL|2=FAX|3=SMS| 4=EMAIL]; NULL-Wert erlaubt.</xs:documentation>
						<xs:appinfo>
							<altova:exampleValues>
								<altova:example value="4"/>
								<altova:example value=""/>
							</altova:exampleValues>
						</xs:appinfo>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:normalizedString">
							<xs:maxLength value="1"/>
							<xs:pattern value="([0-4])?"/>
							<xs:enumeration value="1"/>
							<xs:enumeration value="2"/>
							<xs:enumeration value="3"/>
							<xs:enumeration value="4"/>
							<xs:enumeration value=""/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="NotificationServiceCode" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Art und Zeitpunkt der Avisierungsmeldung gemäss Tabelle 5: Definition Avisierungscodes; NULL-Wert erlaubt.</xs:documentation>
						<xs:appinfo>
							<altova:exampleValues>
								<altova:example value=""/>
								<altova:example value="0"/>
								<altova:example value="1"/>
								<altova:example value="2"/>
								<altova:example value="3"/>
								<altova:example value="256"/>
								<altova:example value="257"/>
								<altova:example value=""/>
							</altova:exampleValues>
						</xs:appinfo>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:pattern value="([0-9]{1,3})?"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="DeliveryDate" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Auslieferdatum, insbesondere im Stückgut-Bereich; NULL-Wert erlaubt.</xs:documentation>
						<xs:appinfo>
							<altova:exampleValues>
								<altova:example value=""/>
								<altova:example value="20140401"/>
							</altova:exampleValues>
						</xs:appinfo>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="8"/>
							<xs:pattern value="((2[0-9][0-9][0-9])((0[0-9])|(1[0-2]))(([0-2][0-9])|(3[0-1])))?"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="DeliveryTimeJIT" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Auslieferzeitpunkt in Stunden/Minuten hh:mm</xs:documentation>
						<xs:appinfo>
							<altova:exampleValues>
								<altova:example value="08:30"/>
								<altova:example value="15:00"/>
								<altova:example value="18:00"/>
								<altova:example value=""/>
							</altova:exampleValues>
						</xs:appinfo>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="5"/>
							<xs:pattern value="(((0[0-9])|(1[0-9])|(2[0-3])):([0-5][0-9]))?"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="DeliveryTimeFrom" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Zuzustellen frühestens ab 08:30 (hh:mm), gilt nur für Frachtsendungen!</xs:documentation>
						<xs:appinfo>
							<altova:exampleValues>
								<altova:example value="08:00"/>
								<altova:example value=""/>
							</altova:exampleValues>
						</xs:appinfo>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="5"/>
							<xs:pattern value="(((0[0-9])|(1[0-9])|(2[0-3])):([0-5][0-9]))?"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="DeliveryTimeTo" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Zuzustellen bis spätestens 18:30 (hh:mm), gilt nur für Frachtsendungen!</xs:documentation>
						<xs:appinfo>
							<altova:exampleValues>
								<altova:example value="11:59"/>
								<altova:example value=""/>
								<altova:example value="23:00"/>
							</altova:exampleValues>
						</xs:appinfo>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="5"/>
							<xs:pattern value="(((0[0-9])|(1[0-9])|(2[0-3])):([0-5][0-9]))?"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="DeliveryPeriodeCode" minOccurs="0">
					<xs:annotation>
						<xs:documentation>1=vormittags; 2=nachmittags; 3=beides-ganztags
(gilt nur für Frachtsendungen)</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:normalizedString">
							<xs:maxLength value="1"/>
							<xs:minLength value="1"/>
							<xs:enumeration value="1"/>
							<xs:enumeration value="2"/>
							<xs:enumeration value="3"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="DeliveryLocation" minOccurs="0">
					<xs:annotation>
						<xs:documentation>ZustellStandort oder spezielle Anweisung in welchem Anbau</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:normalizedString">
							<xs:maxLength value="35"/>
							<xs:minLength value="0"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="CODAmount" type="wab:Float10v2" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Nachnahme-Betrag in CHF (Cash on Delivery) Belegloses Inkasso.</xs:documentation>
						<xs:appinfo>
							<altova:exampleValues>
								<altova:example value="23.15"/>
								<altova:example value="2099.50"/>
							</altova:exampleValues>
						</xs:appinfo>
					</xs:annotation>
				</xs:element>
				<xs:element name="CODAccountNo" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Nachnahme-Kontonummer (Cash on Delivery) Belegloses Verfahren.</xs:documentation>
						<xs:appinfo>
							<altova:exampleValues>
								<altova:example value="010683242"/>
							</altova:exampleValues>
						</xs:appinfo>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:normalizedString">
							<xs:maxLength value="20"/>
							<xs:minLength value="1"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="CODRefNo" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Nachname Referenznummer ohne Leerschlag</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:normalizedString">
							<xs:maxLength value="27"/>
							<xs:minLength value="16"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="FrightShippingFlag" minOccurs="0">
					<xs:annotation>
						<xs:documentation>0 oder leer = keine Frachtsendung (Paketpost - parcel)
1=Ja, Frachtsendung (Stückgut)</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="1"/>
							<xs:enumeration value="0"/>
							<xs:enumeration value="1"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="ShippingInterface" default="WSBC" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Spezielle Versandschnittselle, default WSBC</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:normalizedString">
							<xs:minLength value="1"/>
							<xs:maxLength value="10"/>
							<xs:whiteSpace value="collapse"/>
							<xs:enumeration value="WSBC"/>
							<xs:enumeration value="WEBSTAMP"/>
							<xs:enumeration value="FRIGHT"/>
							<xs:enumeration value="GLSWEB"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="OrderPositions">
		<xs:annotation>
			<xs:documentation>Bestellposition</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="wab:Position" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Position">
		<xs:annotation>
			<xs:documentation>Bestellposition</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="PosNo">
					<xs:annotation>
						<xs:documentation>Positions-Nummer. Je Position ein Datensatz. Vorschlag der Positionssequenz 10, 20, 30</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:integer">
							<xs:totalDigits value="6"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="ArticleNo" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Eindeutige Artikelnummer des Kunden (Unique)</xs:documentation>
						<xs:appinfo>
							<altova:exampleValues>
								<altova:example value="131004001"/>
								<altova:example value="A-123456789012345678901234567890-23"/>
								<altova:example value="66230012"/>
								<altova:example value="AB-233-22341-1"/>
							</altova:exampleValues>
						</xs:appinfo>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:normalizedString">
							<xs:minLength value="1"/>
							<xs:maxLength value="35"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="EAN" minOccurs="0">
					<xs:annotation>
						<xs:documentation>13-stellige EAN Artikelnummer (European ArticleNumber), Muss mit Barcode auf Artikel übereinstimmen.</xs:documentation>
						<xs:appinfo>
							<altova:exampleValues>
								<altova:example value="761012345678901234"/>
								<altova:example value="7610123456789"/>
								<altova:example value="7611600523740"/>
								<altova:example value=""/>
							</altova:exampleValues>
						</xs:appinfo>
					</xs:annotation>
					<xs:simpleType>
						<xs:annotation>
							<xs:appinfo/>
						</xs:annotation>
						<xs:restriction base="wab:EANvalue">
							<xs:maxLength value="18"/>
							<xs:pattern value="([0-9]{8,18})?"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="YCLot" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Chargennummer</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:normalizedString">
							<xs:maxLength value="10"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="Lot" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Lieferanten-Chargennummer</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:normalizedString">
							<xs:maxLength value="15"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="Plant">
					<xs:annotation>
						<xs:documentation>Lager-ID als Werk-ID gemäss Profil des Distanzhändlers</xs:documentation>
						<xs:appinfo>
							<altova:exampleValues>
								<altova:example value="Y001"/>
								<altova:example value="Y004"/>
							</altova:exampleValues>
						</xs:appinfo>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:normalizedString">
							<xs:length value="4"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="Quantity" type="wab:Float13v3">
					<xs:annotation>
						<xs:documentation>Verkaufsmenge</xs:documentation>
						<xs:appinfo>
							<altova:exampleValues>
								<altova:example value="1234567890123.123"/>
							</altova:exampleValues>
						</xs:appinfo>
					</xs:annotation>
				</xs:element>
				<xs:element name="QuantityISO">
					<xs:annotation>
						<xs:documentation>Verkaufs-Mengeneinheit in ISO-Code. Werte gemäss gültiger, mit Kunde vereinbarter Verkaufsmengenheinheiten</xs:documentation>
						<xs:appinfo>
							<altova:exampleValues>
								<altova:example value="KGM"/>
								<altova:example value="GRM"/>
							</altova:exampleValues>
						</xs:appinfo>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:normalizedString">
							<xs:minLength value="2"/>
							<xs:maxLength value="6"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="ShortDescription" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Kurzbeschreibung des Artikels. Wird vorläufig im Standard nicht unterstützt. Nur für spezielle Lieferscheine geplant.</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:normalizedString">
							<xs:maxLength value="40"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="PickingMessage" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Kommisioniertexte. Je Zeile mit einem „*“ unterteilt.</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:normalizedString">
							<xs:maxLength value="132"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="PickingMessageLC" minOccurs="0">
					<xs:annotation>
						<xs:documentation>SprachCode des Meldungstextes: „de“, „fr“, „it“ oder „en“</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:normalizedString">
							<xs:minLength value="2"/>
							<xs:maxLength value="2"/>
							<xs:enumeration value="de"/>
							<xs:enumeration value="fr"/>
							<xs:enumeration value="it"/>
							<xs:enumeration value="en"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="ReturnReason" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Retouren-Grund gemäss YC-Retouren-Code-Liste
z.B. «R06» - Anderer Artikel geliefert als bestellt.</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="3"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="OrderDocuments">
		<xs:annotation>
			<xs:documentation>Lieferbelege</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice minOccurs="0">
				<xs:element ref="wab:OrderDocFilenames" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Nur für XML-Message/Datei Interface: Container für Filenamen des externen Lieferbeleges</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Docs" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Nur für SOAP-Interface: Container für eingebettete Lieferbelege</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="wab:DocType"/>
							<xs:element ref="wab:DocMimeType">
								<xs:annotation>
									<xs:documentation>MIME Typ in der Form als Extention des Lieferbeleges [pdf|pcl] lower case</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element ref="wab:DocStream">
								<xs:annotation>
									<xs:documentation>Binäre Base64 codierte Filedaten des Lieferbeleges</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
			<xs:attribute name="OrderDocumentsFlag" use="required">
				<xs:annotation>
					<xs:documentation>Entscheid: Ausliefer-Dokumente werden mitgeliefert.
1=Ja, Dokument liegt vor und muss in Sendung mitgeliefert werden.
0=Nein, es liegen keine Lieferdokumente vor.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:normalizedString">
						<xs:minLength value="1"/>
						<xs:maxLength value="1"/>
						<xs:enumeration value="1"/>
						<xs:enumeration value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="OrderDocFilename">
		<xs:annotation>
			<xs:documentation>Filenamen des Lieferbelegs</xs:documentation>
			<xs:appinfo>
				<altova:exampleValues>
					<altova:example value="Easyshop77_DN20141001_141001.pdf"/>
					<altova:example value="Shop_ZS4690073511_130927.pcl"/>
					<altova:example value="Shop_LS1310004001_131004.pdf"/>
					<altova:example value="Easyshop77_IV4690073511_141001.pdf"/>
					<altova:example value="Easyshop77_ZS4690073511_141001.pdf"/>
					<altova:example value="Easyshop77_DN4690073511-1_141001.pdf"/>
					<altova:example value="Easyshop77_DN4690073511-2_141008.pdf"/>
				</altova:exampleValues>
			</xs:appinfo>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:normalizedString">
				<xs:maxLength value="50"/>
				<xs:whiteSpace value="collapse"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="OrderDocFilenames">
		<xs:annotation>
			<xs:documentation>Container für Filenamen der Lieferbelege</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="wab:OrderDocFilename" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
